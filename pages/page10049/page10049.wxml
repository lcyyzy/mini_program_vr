  <block wx:if="{{real_value}}">
    <template is="zcText" data="{{data:data.content[2], real_value:real_value.form_data.time, detail_data: detail_data}}"></template>
  </block>
  <block wx:elif="{{detail_data.time && !vessel_page_data.form_data.time}}">
    <template is="zcText" data="{{data:data.content[2], real_value:detail_data.time, detail_data: detail_data}}"></template>
  </block>
  <block wx:elif="{{vessel_page_data.form_data.time}}">
    <template is="zcText" data="{{data:data.content[2], real_value:vessel_page_data.form_data.time, detail_data: detail_data}}"></template>
  </block>
  <block wx:else>
