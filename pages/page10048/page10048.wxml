              <template is="wxParse" data="{{wxParseData: real_value}}"/>
            </view>
          </label>
        </block>
      </block>
      <block wx:else>
        <block wx:if="{{data.customFeature.action == 'share' || data.customFeature.action == 'contact'}}">
          <button class="element button" open-type="{{data.customFeature.action}}" style="{{data.style}}" data-desc="{{data.customFeature.desc}}" catchtap="onShareAppMessage">
            {{real_value}}
          </button>
        </block>
        <block wx:else>
          <label class="element button" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
            {{real_value}}
          </label>
        </block>
      </block>
    </block>
    <block wx:else>
      <block wx:if="{{data.customFeature.action == 'share' || data.customFeature.action == 'contact'}}">
        <button class="element button" open-type="{{data.customFeature.action}}" style="{{data.style}}" data-desc="{{data.customFeature.desc}}" catchtap="onShareAppMessage">
          {{data.content}}
        </button>
      </block>
      <block wx:else>
        <label class="element button" style="{{data.style}}" data-event-params="{{data.eventParams}}" catchtap="{{data.eventHandler}}">
          {{data.content}}
        </label>
      </block>
    </block>
  </template>
<block wx:if="{{real_value}}">
  <template is="zcButton" data="{{data:data.content[0]}}"></template>
</block>
<block wx:elif="{{detail_data && !vessel_page_data.form_data}}">
  <template is="zcButton" data="{{data:data.content[0]}}"></template>
</block>
<block wx:elif="{{vessel_page_data.form_data}}">
  <template is="zcButton" data="{{data:data.content[0]}}"></template>
</block>
<block wx:else>
  <template is="zcButton" data="{{data:data.content[0]}}"></template>
</block>

  </view>
</template>
<view class="ele-container">
<block wx:if="{{real_value}}">
